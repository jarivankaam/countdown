{
  "name": "framework-boilerplate",
  "version": "1.0.0",
  "description": "this project is made to simplify and speed up the process of creating a `new project`. It is very loosely based on a framework I use in my internship. It is not meant to be used as a framework, but as a starting point for your `own project`.",
  "main": "index.js",
  "scripts": {
    "watch:scss": "sass --watch private/scss/style.scss  ./public/css/main.css",
    "watch:ts": "tsc -w",
    "test": "snyk test",
    "monitor": "snyk monitor",
    "pack": "webpack",
    "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jarivankaam/framework-boilerplate.git"
  },
