  "bugs": {
    "url": "https://github.com/jarivankaam/framework-boilerplate/issues"
  },
  "homepage": "https://github.com/jarivankaam/framework-boilerplate#readme",
  "devDependencies": {
    "@types/jquery": "^3.5.30",
    "@types/node": "^18.11.18",
    "css-loader": "^6.11.0",
    "cssnano": "^5.1.14",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^7.0.2",
    "sass": "^1.77.8",
    "sass-loader": "^13.3.3",
    "snyk": "^1.1085.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.4",
